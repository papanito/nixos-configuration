{
  "resource": {
    "hcloud_firewall": {
      "standard": {
        "name": "standard-fw",
        "rule": [
          {
            "direction": "in",
            "port": "22",
            "protocol": "tcp",
            "source_ips": [
              "0.0.0.0/0"
            ]
          },
          {
            "direction": "in",
            "port": "443",
            "protocol": "tcp",
            "source_ips": [
              "0.0.0.0/0"
            ]
          }
        ]
      }
    },
    "hcloud_network": {
      "private_vlan": {
        "ip_range": "10.0.0.0/16",
        "name": "nixos-private"
      }
    }
  },
  "terraform": {
    "backend": {
      "local": {
        "path": "terraform.tfstate"
      }
    },
    "required_providers": {
      "hcloud": {
        "source": "hetznercloud/hcloud"
      }
    }
  }
}
